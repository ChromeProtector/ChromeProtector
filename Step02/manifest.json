{
  "name": "ChromeProtector",
  "manifest_version": 3,
  "version": "0.2",
  "description": "An extension for Chrome that aims to protect (as much as possible) against typosquatting attacks",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
	"https://*/*",
	"http://*/*"
  ],
  "action": {
    "default_icon": {
      "32": "icons/ok.png"
    }
  },
  "options_page": "options.html"

}